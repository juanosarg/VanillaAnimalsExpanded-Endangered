<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Genetic Rim</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>


			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesBear"]/ingredients/li/filter/thingDefs</xpath>
				<value>
					<li>Corpse_AEXP_Panda</li>
					

				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesBear"]/fixedIngredientFilter/thingDefs</xpath>
				<value>
					<li>Corpse_AEXP_Panda</li>
					

				</value>
			</li>	
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesWolf"]/ingredients/li/filter/thingDefs</xpath>
				<value>
					<li>Corpse_AEXP_AfricanWildDog</li>
					

				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesWolf"]/fixedIngredientFilter/thingDefs</xpath>
				<value>
					<li>Corpse_AEXP_AfricanWildDog</li>
					

				</value>
			</li>	
			
			
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesChicken"]/ingredients/li/filter/thingDefs</xpath>
				<value>
					<li>Corpse_AEXP_Moa</li>
					<li>Corpse_AEXP_RockhopperPenguin</li>
					

				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesChicken"]/fixedIngredientFilter/thingDefs</xpath>
				<value>
					<li>Corpse_AEXP_Moa</li>
					<li>Corpse_AEXP_RockhopperPenguin</li>

				</value>
			</li>

		




		</operations>
		</match>
	</Operation>


</Patch>

